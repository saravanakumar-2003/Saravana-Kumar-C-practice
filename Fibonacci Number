Fibonacci Number
#include<iostream>
using namespace std;
#include<cmath>
int main()
{
    int n,a,b;
    double root,root1;
    cin >> n;
    root=sqrt(5*n*n+4);
    a=root;
    root1=sqrt(5*n*n-4);
    b=root1;
    if(root==a || root1==b)
    {
        cout << "Fibonacci number";
    }
    else
    {
        cout << "Not Fibonacci number";
    }
   
}
