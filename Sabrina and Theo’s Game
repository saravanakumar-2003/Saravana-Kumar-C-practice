Sabrina and Theoâ€™s Game
#include<iostream>
using namespace std;
int main()
{
    int m,y;
    cin>>m>>y;
    if(m==2)
    {
        if(y%100==0 && y%400==0)
        {
            cout<<"Number of days is 29";
        }
        else if(y%4==0)
        {
            cout<<"Number of days is 29";
        }
        else
        {
            cout<<"Number of days is 28";
        }
    }
    else
    {
        if(m==1 || m==3 || m==5 || m==7 || m==8 || m==10 || m==12)
        {
            cout<<"Number of days is 31";
        }
        else if(m==2 || m==4 ||m==6 || m==9 ||m==11)
        {
            cout<<"Number of days is 30";
        }
    }
    return 0;
}
