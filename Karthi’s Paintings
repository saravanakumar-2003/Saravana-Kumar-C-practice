Karthiâ€™s Paintings
#include<iostream>
using namespace std;
int main()
{
    int l,b,l1,b1,l2,b2,a1,a2,a3;
    cin>>l>>b>>l1>>b1>>l2>>b2;
    a1=l*b;
    a2=l1*b1;
    a3=l2*b2;
    if(a1-(a2+a3)>=0)
    {
        cout<<"Karthi can fix both painting";
    }
    else
    {
        cout<<"Karthi cannot fix both painting";
    }
    return 0;
}
